/**********************************************************************************************************************
* Name         : CM_DEM_M_SIZEOF_EVENTENTRY_J1939FREEZEFRAME
* Parameter    : None
* Return value : None
* Description  : For each EventEntry referenced in Table Dem_Cfg_EventEntryPtr (Readout Buffer Entries are not tested):
*                Verify that J1939FreezeFrame[] is valid for values in range [0..(DEM_CFG_MAX_J1939_FF_SIZE + DEM_CFG_MAX_J1939_EXP_FF_SIZE)[,
*                if J1939 freezeframe or J1939Expanded freezeframe is supported.
* Requirements :
**********************************************************************************************************************/
def CM_DEM_M_SIZEOF_EVENTENTRY_J1939FREEZEFRAME()
{
  var PrimaryEntry0Tested = false
  // Array("") will report an error if the object does not exist
  var Dem_Cfg_EventEntryPtrTable = Array("Dem_Cfg_EventEntryPtr")

  for(var Index = 0; Index < Dem_Cfg_EventEntryPtrTable.GetSize(); ++Index)
  {
    var ArrayElement = Dem_Cfg_EventEntryPtrTable.GetElemAsString(Index)

    // Extract name of the global variable (e.g., "Dem_Cfg_PrimaryEntry_0"), to which the entry of Dem_Cfg_EventEntryPtr points
    var Dem_EventEntry = GetStructNameFromReferenceString(ArrayElement)

    // At least one Primary Entry is always configured, so table should always contain a pointer to "Dem_Cfg_PrimaryEntry_0"
    if (Dem_EventEntry == "Dem_Cfg_PrimaryEntry_0")
    {
      PrimaryEntry0Tested = true
    }
    // Assert that the name of the referenced struct starts with 'Dem_Cfg'
    CheckVariableInDemNameSpace(Dem_EventEntry)

    // Only event memory entries in Dem_Cfg_EventEntryPtr are tested
    // Readout buffer entries are verified by customer with SMI-524392
    if(Dem_EventEntry.find("Dem_Cfg_ReadoutBuffer") < 0)
    {
      var Dem_EventEntryStruct = Struct(Dem_EventEntry)

      if(Dem_EventEntryStruct.HasMember("J1939FreezeFrame"))
      {
        var DEM_CFG_MAX_J1939_FF_SIZE = Define("DEM_CFG_MAX_J1939_FF_SIZE")
        var DEM_CFG_MAX_J1939_EXP_FF_SIZE = Define("DEM_CFG_MAX_J1939_EXP_FF_SIZE")
        var TotalSize = DEM_CFG_MAX_J1939_FF_SIZE.GetValueAsNumber() + DEM_CFG_MAX_J1939_EXP_FF_SIZE.GetValueAsNumber()
        var Dem_EventEntryStruct_J1939FreezeFrame = Dem_EventEntryStruct.GetMemberAsArray("J1939FreezeFrame")

        AssertArraySize(Dem_EventEntryStruct_J1939FreezeFrame, Equal, TotalSize)

      }
      else if (IsStdOff("DEM_CFG_SUPPORT_J1939_FREEZEFRAME") && IsStdOff("DEM_CFG_SUPPORT_J1939_EXPANDED_FREEZEFRAME"))
      {
        // Feature disabled, no check needed
      }
      else
      {
        ReportError("Member J1939FreezeFrame not generated or renamed for EventEntries although DEM_CFG_SUPPORT_J1939_FREEZEFRAME or DEM_CFG_SUPPORT_J1939_EXPANDED_FREEZEFRAME is ON or one of the PP switches does not exist.")
      }
    }
  }
  if (PrimaryEntry0Tested == false)
  {
    ReportError("Dem_Cfg_EventEntryPtr does not contain reference to Dem_Cfg_PrimaryEntry_0!")
  }
}

