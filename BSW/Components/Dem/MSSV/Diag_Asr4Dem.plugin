/**********************************************************************************************************************
* Name         : CheckEntryTimeSeriesSnapshotDataTableElement
* Parameter    : array - the array object for which the function is called
* Parameter    : index - the index of the current array element
* Parameter    : value - the value of the current array element
* Return value : None
* Description  : Callback function that verifies for an element in array Dem_Cfg_TimeSeriesSnapshotData
*                the array referenced by EntryData has a size of BufferSize bytes
*                the inner dimension of the array referenced by EntryData has MaxSnapShotSize elements
* Requirements : N/A
**********************************************************************************************************************/
def CheckEntryTimeSeriesSnapshotDataTableElement(array, index, value)
{
  var EntryDataPtrString = value.GetMemberAsString("EntryDataOfTimeSeriesEntryTable")
  var ArrayByteSize = value.GetMemberAsNumber("BufferSizeOfTimeSeriesEntryTable");
  var InnerArraySize = value.GetMemberAsNumber("MaxSnapShotSizeOfTimeSeriesEntryTable");

  var StructName=GetStructNameFromStructMemberReferenceString(EntryDataPtrString)
  /* Assert that the name of the referenced struct starts with 'Dem_Cfg' */
  CheckVariableInDemNameSpace(StructName)

  var TimeSeriesEntryStruct = Struct(StructName)

  var memberName = GetMemberNameFromStructMemberReferenceString(EntryDataPtrString);
  var SnapshotDataArray = TimeSeriesEntryStruct.GetMemberAsArray(memberName)
  var InnerArray = SnapshotDataArray.GetElemAsArray(0)

  Assert(SnapshotDataArray.GetSize() * InnerArraySize, Equal, ArrayByteSize)
  AssertArraySize(InnerArray, Equal, InnerArraySize)
}

/**********************************************************************************************************************
* Name         : GetBaseEntryFromTimeSeriesBaseEntryPtrString
* Parameter    : TimeSeriesBaseEntryPtrString - base entry ptr string
* Parameter    : TestAssertions - Condition to make assertions that don't need to be duplicated for every caller of this function
* Return value : BaseEntry
* Description  : Get base entry from time series base entry ptr string
* Requirements : N/A
**********************************************************************************************************************/
def GetBaseEntryFromTimeSeriesBaseEntryPtrString(TimeSeriesBaseEntryPtrString, TestAssertions) {
  var StructName=GetStructNameFromStructMemberReferenceString(TimeSeriesBaseEntryPtrString)
  /* Assert that the name of the referenced struct starts with 'Dem_Cfg' */

  if(TestAssertions) {
    CheckVariableInDemNameSpace(StructName)
  }

  var TimeSeriesEntryStruct = Struct(StructName)

  var BaseEntryMemberName = GetMemberNameFromStructMemberReferenceString(TimeSeriesBaseEntryPtrString);

  Assert(BaseEntryMemberName, Equal, "BaseEntry")
  return TimeSeriesEntryStruct.GetMemberAsStruct(BaseEntryMemberName)
}

/**********************************************************************************************************************
* Name         : CheckBaseEntryInTimeSeriesEntryTableElement
* Parameter    : array - the array object for which the function is called
* Parameter    : index - the index of the current array element
* Parameter    : value - the value of the current array element
* Return value : None
* Description  : Callback function that verifies for the BaseEntry referenced by BaseEntryOfTimeSeriesEntryTable
*                of an element in array Dem_Cfg_TimeSeriesEntryTable:
*                - that BaseEntry.UnusedSamples[][] is defined where the first index has to be in the range
*                [0.. DEM_CFG_TIMESERIES_MAX_NUMBER_OF_SERIES[ and the second index in the range
*                [0.. DEM_CFG_TIMESERIES_MAX_NUMBER_OF_SETS[
*                - that BaseEntry.UnusedPastSamples[][] is defined where the first index has to be in the range
*                [0.. DEM_CFG_TIMESERIES_MAX_NUMBER_OF_SERIES[ and the second index in the range
*                [0.. DEM_CFG_TIMESERIES_MAX_NUMBER_OF_SETS[
* Requirements : N/A
**********************************************************************************************************************/
def CheckBaseEntryInTimeSeriesEntryTableElement(array, index, value)
{
  var TimeSeriesBaseEntryPtrString = value.GetMemberAsString("BaseEntryOfTimeSeriesEntryTable")
  var BaseEntry = GetBaseEntryFromTimeSeriesBaseEntryPtrString(TimeSeriesBaseEntryPtrString, true)

  if(BaseEntry.HasMember("UnusedPastSamples"))
  {
    var DEM_CFG_TIMESERIES_MAX_NUMBER_OF_SETS = Define("DEM_CFG_TIMESERIES_MAX_NUMBER_OF_SETS")
    var DEM_CFG_TIMESERIES_MAX_NUMBER_OF_SERIES = Define("DEM_CFG_TIMESERIES_MAX_NUMBER_OF_SERIES")

    var UnusedPastSamples_Buffer = BaseEntry.GetMemberAsArray("UnusedPastSamples")
    var InnerArray = UnusedPastSamples_Buffer.GetElemAsArray(0)

    AssertArraySize(UnusedPastSamples_Buffer, Equal, DEM_CFG_TIMESERIES_MAX_NUMBER_OF_SERIES)
    AssertArraySize(InnerArray, Equal, DEM_CFG_TIMESERIES_MAX_NUMBER_OF_SETS)
  }
  else if (IsStdOff("DEM_CFG_SUPPORT_TIME_SERIES_PAST"))
  {
    // Feature is disabled, no check needed
  }
  else
  {
    ReportError("Dem_TimeSeriesEntryType.BaseEntry.UnusedPastSamples not generated although DEM_CFG_SUPPORT_TIME_SERIES_PAST is ON or DEM_CFG_SUPPORT_TIME_SERIES_PAST does not exist.")
  }

  if (BaseEntry.HasMember("UnusedFutureSamples"))
  {
    var DEM_CFG_TIMESERIES_MAX_NUMBER_OF_SETS = Define("DEM_CFG_TIMESERIES_MAX_NUMBER_OF_SETS")
    var DEM_CFG_TIMESERIES_MAX_NUMBER_OF_SERIES = Define("DEM_CFG_TIMESERIES_MAX_NUMBER_OF_SERIES")

    var UnusedFutureSamples_Buffer = BaseEntry.GetMemberAsArray("UnusedFutureSamples")
    var InnerArray = UnusedFutureSamples_Buffer.GetElemAsArray(0)

    AssertArraySize(UnusedFutureSamples_Buffer, Equal, DEM_CFG_TIMESERIES_MAX_NUMBER_OF_SERIES)
    AssertArraySize(InnerArray, Equal, DEM_CFG_TIMESERIES_MAX_NUMBER_OF_SETS)
  }
  else if (IsStdOff("DEM_CFG_SUPPORT_TIME_SERIES_FUTURE"))
  {
    // Feature disabled, no check needed
  }
  else
  {
    ReportError("Dem_TimeSeriesEntryType.BaseEntry.UnusedFutureSamples[][] not generated although DEM_CFG_SUPPORT_TIME_SERIES_FUTURE is ON or DEM_CFG_SUPPORT_TIME_SERIES_FUTURE does not exist.")
  }
}

