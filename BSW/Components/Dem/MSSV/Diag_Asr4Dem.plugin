/**********************************************************************************************************************
* Name         : CM_DEM_M_SIZEOF_EVENTENTRY_EXTENDEDDATA
* Parameter    : None
* Return value : None
* Description  : For each Event Entry referenced in Table Dem_Cfg_EventEntryPtr (Readout Buffer Entries are not tested):
*                Verify that ExtendedData[] is valid for values in range [0..DEM_CFG_MAX_NUM_EXT_RECORDS[
*                Verify that each ExtendedData[][] has at least DEM_CFG_MAX_SIZE_EXT_RECORD elements
* Requirements :
**********************************************************************************************************************/
def CM_DEM_M_SIZEOF_EVENTENTRY_EXTENDEDDATA()
{
  var PrimaryEntry0Tested = false
  // Array("") will report an error if the object does not exist
  var Dem_Cfg_EventEntryPtrTable = Array("Dem_Cfg_EventEntryPtr")

  for(var Index = 0; Index < Dem_Cfg_EventEntryPtrTable.GetSize(); ++Index)
  {
    var ArrayElement = Dem_Cfg_EventEntryPtrTable.GetElemAsString(Index)

    // Extract name of the global variable (e.g., "Dem_Cfg_PrimaryEntry_0"), to which the entry of Dem_Cfg_EventEntryPtr points
    var Dem_EventEntry = GetStructNameFromReferenceString(ArrayElement)

    // At least one Primary Entry is always configured, so table should always contain a pointer to "Dem_Cfg_PrimaryEntry_0"
    if (Dem_EventEntry == "Dem_Cfg_PrimaryEntry_0")
    {
      PrimaryEntry0Tested = true
    }
    // Assert that the name of the referenced struct starts with 'Dem_Cfg'
    CheckVariableInDemNameSpace(Dem_EventEntry)

    // Only event memory entries in Dem_Cfg_EventEntryPtr are tested
    // Readout buffer entries are verified by customer with SMI-524392
    if(Dem_EventEntry.find("Dem_Cfg_ReadoutBuffer") < 0)
    {
      var Dem_EventEntryStruct = Struct(Dem_EventEntry)

      if(Dem_EventEntryStruct.HasMember("ExtendedData"))
      {
        var DEM_CFG_MAX_NUM_EXT_RECORDS = Define("DEM_CFG_MAX_NUM_EXT_RECORDS")
        var DEM_CFG_MAX_SIZE_EXT_RECORD = Define("DEM_CFG_MAX_SIZE_EXT_RECORD")

        var Dem_EventEntry_ExtendedData = Dem_EventEntryStruct.GetMemberAsArray("ExtendedData")
        var Dem_EventEntry_ExtendedData_Max_Size = Dem_EventEntry_ExtendedData.GetElemAsArray(0)

        AssertArraySize(Dem_EventEntry_ExtendedData, Equal, DEM_CFG_MAX_NUM_EXT_RECORDS)
        AssertArraySize(Dem_EventEntry_ExtendedData_Max_Size, Equal, DEM_CFG_MAX_SIZE_EXT_RECORD)

      }
      else if (IsStdOff("DEM_CFG_SUPPORT_USER_ERECS"))
      {
        // Feature disabled, no check needed
      }
      else
      {
        ReportError("Member ExtendedData not generated or renamed for EventEntries although DEM_CFG_SUPPORT_USER_ERECS is ON or PP switch does not exist.")
      }
    }
  }
  if (PrimaryEntry0Tested == false)
  {
    ReportError("Dem_Cfg_EventEntryPtr does not contain reference to Dem_Cfg_PrimaryEntry_0!")
  }
}

