    // Feature disabled, no check needed
  }
  else
  {
    ReportError("Dem_Cfg_TimeSeriesEntryTable not present although DEM_CFG_SUPPORT_TIME_SERIES_PAST/FUTURE is ON or one of the PP switches does not exist.")
  }
