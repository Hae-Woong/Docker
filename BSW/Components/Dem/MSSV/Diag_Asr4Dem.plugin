/**********************************************************************************************************************
* Name         : CM_DEM_M_SIZEOF_CUSTOMTRIGGERENTRY_SNAPSHOTDATA
* Parameter    : None
* Return value : None
* Description  : For each CustomTriggerEntry referenced in Table Dem_Cfg_CustomTriggerEntryPtr (Readout Buffer Entries are not tested):
*                Verify that SnapshotData[] is defined for values in range [0..DEM_CFG_MAX_SIZE_CUSTOM_TRIGGERED_RECORD[
*                Verify that TSSnapshotData[][] is defined where the first index has to be in the range [0..DEM_CFG_MAX_SAMPLES_CUSTOM_TRIGGERED_TIMESERIESSNAPSHOT[ 
*                                                                and second index in range [0..DEM_CFG_MAX_SIZE_CUSTOM_TRIGGERED_RECORD[
* Requirements :
**********************************************************************************************************************/
def CM_DEM_M_SIZEOF_CUSTOMTRIGGERENTRY_SNAPSHOTDATA()
{
  if(SymbolExists("Dem_Cfg_CustomTriggerEntryPtr"))
  {
    var AtleastOneEntryTested = false
    // Array("") will report an error if the object does not exist
    var Dem_Cfg_CustomTriggerEntryPtrTable = Array("Dem_Cfg_CustomTriggerEntryPtr")

    for(var Index = 0; Index < Dem_Cfg_CustomTriggerEntryPtrTable.GetSize(); ++Index)
    {
      var ArrayElement = Dem_Cfg_CustomTriggerEntryPtrTable.GetElemAsString(Index)

      // Extract name of the global variable (e.g., "Dem_Cfg_PrimaryCustomTriggerEntry_0"), to which the entry of Dem_Cfg_CustomTriggerEntryPtrTable points
      var Dem_CustomTriggerEntry = GetStructNameFromReferenceString(ArrayElement)

      // Assert that the name of the referenced struct starts with 'Dem_Cfg'
      CheckVariableInDemNameSpace(Dem_CustomTriggerEntry)

      // Only primary custom trigger memory and user defined custom trigger memory entries in Dem_Cfg_CustomTriggerEntryPtr are tested
      // Readout buffer entries are verified by customer with SMI-DEM-1000
      if(Dem_CustomTriggerEntry.find("Dem_Cfg_ReadoutBuffer") < 0)
      {
        // Atleast one entry with reference to either PrimaryMemory or UserDefinedMemory is tested
        AtleastOneEntryTested = true
        var Dem_CustomTriggerEntryStruct = Struct(Dem_CustomTriggerEntry)

        // For Configured SRecs with Custom Trigger
        if(Dem_CustomTriggerEntryStruct.HasMember("SnapshotData"))
        {
          var DEM_CFG_MAX_SIZE_CUSTOM_TRIGGERED_RECORD = Define("DEM_CFG_MAX_SIZE_CUSTOM_TRIGGERED_RECORD")
          var Dem_CustomTriggerEntryStruct_SnapshotData = Dem_CustomTriggerEntryStruct.GetMemberAsArray("SnapshotData")
        
          AssertArraySize(Dem_CustomTriggerEntryStruct_SnapshotData, Equal, DEM_CFG_MAX_SIZE_CUSTOM_TRIGGERED_RECORD)
        }
        else if (IsStdOff("DEM_CFG_SUPPORT_CUSTOMTRIGGER_SREC"))
        {
          // Feature disabled, no check needed : No Configured SRecs with Custom Trigger
        }
        else
        {
          ReportError("Member SnapshotData not generated or renamed for CustomTriggerEntries in Dem_Cfg_CustomTriggerEntryPtr[], even though the switch DEM_CFG_SUPPORT_CUSTOMTRIGGER_SREC is ON or PP switch does not exist.")
        }
        
        // For TimeSeries SRecs with Custom Trigger
        if(Dem_CustomTriggerEntryStruct.HasMember("TSSnapshotData"))
        {
          var DEM_CFG_MAX_SAMPLES_CUSTOM_TRIGGERED_TIMESERIESSNAPSHOT = Define("DEM_CFG_MAX_SAMPLES_CUSTOM_TRIGGERED_TIMESERIESSNAPSHOT")
          var DEM_CFG_MAX_SIZE_CUSTOM_TRIGGERED_RECORD = Define("DEM_CFG_MAX_SIZE_CUSTOM_TRIGGERED_RECORD")

          var Dem_CustomTriggerEntryStruct_TSSnapshotData = Dem_CustomTriggerEntryStruct.GetMemberAsArray("TSSnapshotData")
          var InnerArray = Dem_CustomTriggerEntryStruct_TSSnapshotData.GetElemAsArray(0)

          AssertArraySize(Dem_CustomTriggerEntryStruct_TSSnapshotData, Equal, DEM_CFG_MAX_SAMPLES_CUSTOM_TRIGGERED_TIMESERIESSNAPSHOT)
          AssertArraySize(InnerArray, Equal, DEM_CFG_MAX_SIZE_CUSTOM_TRIGGERED_RECORD)
        }
        else if (IsStdOff("DEM_CFG_SUPPORT_CUSTOMTRIGGER_TSSR"))
        {
          // Feature disabled, no check needed : No TimeSeries SRecs with Custom Trigger
        }
        else
        {
          ReportError("Member TSSnapshotData not generated or renamed for CustomTriggerEntries in Dem_Cfg_CustomTriggerEntryPtr[], even though the switch DEM_CFG_SUPPORT_CUSTOMTRIGGER_TSSR is ON or PP switch does not exist.")
        }
      }
    }
    if (AtleastOneEntryTested == false)
    {
      ReportError("Dem_Cfg_CustomTriggerEntryPtr does not contain reference to either PrimaryMemory or UserDefinedMemory Custom Trigger Entry.!!")
    }
  }
  else if ((IsStdOff("DEM_CFG_SUPPORT_CUSTOMTRIGGER_SREC")) && (IsStdOff("DEM_CFG_SUPPORT_CUSTOMTRIGGER_TSSR")))
  {
    // Feature disabled, no check needed
  }
  else
  {
    ReportError("Dem_Cfg_CustomTriggerEntryPtr renamed or not generated although DEM_CFG_SUPPORT_CUSTOMTRIGGER_SREC or DEM_CFG_SUPPORT_CUSTOMTRIGGER_TSSR is ON or PP switches do not exist.")
  }
}

