/**********************************************************************************************************************
* Name         : CM_DEM_M_SIZEOF_PRESTORED_SNAPSHOT
* Parameter    : None
* Return value : None
* Description  : Verify that Dem_Cfg_PrestorageEntryType.SnapshotData[] has at least DEM_CFG_MAX_SIZE_SNAPSHOT elements
* Requirements :
**********************************************************************************************************************/
def CM_DEM_M_SIZEOF_PRESTORED_SNAPSHOT()
{
  if (SymbolExists("Dem_Cfg_Prestorage") && Array("Dem_Cfg_Prestorage").GetElemAsStruct(0).HasMember("SnapshotData"))
  {
    var Dem_Cfg_Prestorage = Array("Dem_Cfg_Prestorage")
    var Dem_Cfg_PrestorageEntry = Dem_Cfg_Prestorage.GetElemAsStruct(0)
    var DEM_CFG_MAX_SIZE_SNAPSHOT = Define("DEM_CFG_MAX_SIZE_SNAPSHOT")
    var Dem_Cfg_PrestorageEntry_SnapshotData = Dem_Cfg_PrestorageEntry.GetMemberAsArray("SnapshotData")
    AssertArraySize(Dem_Cfg_PrestorageEntry_SnapshotData, Equal, DEM_CFG_MAX_SIZE_SNAPSHOT)
  }
  else if (IsStdOff("DEM_CFG_SUPPORT_PRESTORED_FF") || IsStdOff("DEM_CFG_SUPPORT_SRECS"))
  {
    // Feature is disabled, no check needed
  }
  else
  {
    ReportError("Dem_Cfg_Prestorage or Member SnapshotData in Dem_Cfg_Prestorage does not exist although DEM_CFG_SUPPORT_PRESTORED_FF and DEM_CFG_SUPPORT_SRECS both are on or one of the PP switches does not exist.")
  }
}

/**********************************************************************************************************************
* Name         : CM_DEM_M_SIZEOF_PRESTORED_OBDFREEZEFRAME
* Parameter    : None
* Return value : None
* Description  : Verify that Dem_Cfg_PrestorageEntryType.ObdFreezeFrame[] has at least DEM_CFG_MAX_SIZE_OBDFREEZEFRAME
*                elements if OBD is supported.
* Requirements :
**********************************************************************************************************************/
def CM_DEM_M_SIZEOF_PRESTORED_OBDFREEZEFRAME()
{
  if (SymbolExists("Dem_Cfg_Prestorage") && Array("Dem_Cfg_Prestorage").GetElemAsStruct(0).HasMember("ObdFreezeFrame"))
  {
    var Dem_Cfg_Prestorage = Array("Dem_Cfg_Prestorage")
    var Dem_Cfg_PrestorageEntry = Dem_Cfg_Prestorage.GetElemAsStruct(0)
    var DEM_CFG_MAX_SIZE_OBDFREEZEFRAME = Define("DEM_CFG_MAX_SIZE_OBDFREEZEFRAME")
    var Dem_Cfg_PrestorageEntry_ObdFreezeFrame = Dem_Cfg_PrestorageEntry.GetMemberAsArray("ObdFreezeFrame")
    AssertArraySize(Dem_Cfg_PrestorageEntry_ObdFreezeFrame, Equal, DEM_CFG_MAX_SIZE_OBDFREEZEFRAME)
  }
  else if (IsStdOff("DEM_CFG_SUPPORT_PRESTORED_FF") || IsStdOff("DEM_CFG_SUPPORT_OBDII_OR_OBDONUDS"))
