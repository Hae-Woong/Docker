/**********************************************************************************************************************
* Name         : GetStructNameFromStructMemberReferenceString
* Parameter    : RefString
* Return value : The name of the referenced Struct variable
* Description  : Extracts the name of the Struct variable from the passed String which is a reference to a member of
*                the Struct.
*                E.g., extracts "Dem_Cfg_PrimaryTimeSeriesEntry_0" from
*                "&(Dem_Cfg_PrimaryTimeSeriesEntry_0.SnapshotData_NormalRate[0][0])"
* Requirements : N/A
**********************************************************************************************************************/
def GetStructNameFromStructMemberReferenceString(RefString)
{
  var RefStart = "&("
  RefString=drop(RefString, RefStart.size())

  var DotIndex = RefString.find(".")

  return take(RefString, DotIndex)
}

/**********************************************************************************************************************
* Name         : GetMemberNameFromStructMemberReferenceString
* Parameter    : RefString
* Return value : The name of the referenced Struct member variable
* Description  : Extracts the name of the Struct member variable from the passed String which is a reference to the
*                Struct member.
*                E.g., extracts "SnapshotData_NormalRate" from
*                "&(Dem_Cfg_PrimaryTimeSeriesEntry_0.SnapshotData_NormalRate[0][0])"
* Requirements : N/A
**********************************************************************************************************************/
def GetMemberNameFromStructMemberReferenceString(RefString)
{
  var DotIndex = RefString.find(".")
  RefString=drop(RefString, DotIndex+1)

  var RefEnd = ")"
  var TwoDimArrayRefEnd = "[0][0])"

  if(RefString.find(TwoDimArrayRefEnd) >= 0)
  {
    RefString=take(RefString, RefString.size()- TwoDimArrayRefEnd.size())
  }
  else if(RefString.find(RefEnd) >= 0)
  {
    RefString=take(RefString, RefString.size()- RefEnd.size())
  }

  return RefString
}

/**********************************************************************************************************************
* Name         : GetStructNameFromReferenceString
* Parameter    : RefString
* Return value : The name of the referenced Struct variable
* Description  : Extracts the name of the Struct variable from the passed String which is a reference to the Struct.
*                E.g. extracts "Dem_Cfg_PrimaryEntry_0" from "(&(Dem_Cfg_PrimaryEntry_0))"
* Requirements : N/A
**********************************************************************************************************************/
def GetStructNameFromReferenceString(RefString)
{
  var RefStart = "Dem"
  var RefEnd = ")"

  if(RefString.find(RefEnd) >= 0)
  {
    RefString=take(RefString, RefString.find(RefEnd))
  }
  if(RefString.find(RefStart) >= 0)
  {
    RefString = drop(RefString, RefString.find(RefStart))
  }

  return RefString
}

/**********************************************************************************************************************
* Name         : CheckVariableInDemNameSpace
* Parameter    : RefString
* Return value : None
* Description  : Verify that the variable name is defined in DEM namespace (starts with 'Dem_Cfg')
* Requirements : N/A
**********************************************************************************************************************/
def CheckVariableInDemNameSpace(RefString)
{
    /* Assert that the name of the referenced variable with 'Dem_Cfg' */
    Assert(RefString.find("Dem_Cfg_") == 0, RefString + " starts with", Equal, "Dem_Cfg_")
}

/**********************************************************************************************************************
* Name         : CheckQMDefines
* Parameter    : None
* Return value : None
* Description  : This rule checks the setting of QM-related preprocessor defines.
*                Typically it checks that QM features are inactive in safety context.
* Requirements : N/A
**********************************************************************************************************************/
def CheckQMDefines()
{
  // SMI-1002
  AssertDefineIsStdOff("DEM_CFG_SUPPORT_MIRROR")
  // SMI-1001
  AssertDefineIsStdOff("DEM_CFG_SUPPORT_CALIBRATION")
}

/**********************************************************************************************************************
* Name         : CheckSilentDefines
* Parameter    : None
* Return value : None
* Description  : Verify that all features that guarantee safe runtime are activated.
* Requirements : N/A
**********************************************************************************************************************/
def CheckSilentDefines()
{
  AssertDefineIsStdOn("DEM_CFG_SAFE_BSW_MODE")
  AssertDefineIsStdOn("DEM_DEV_ERROR_DETECT")
}

/**********************************************************************************************************************
* Name         : CM_DEM_M_SIZEOF_ADMINDATA_CYCLECOUNTER
* Parameter    : None
* Return value : None
* Description  : Verify Dem_Cfg_AdminData.CycleCounter[] is defined for each id in range [0..DEM_CFG_GLOBAL_CYCLE_COUNT[
* Requirements :
**********************************************************************************************************************/
def CM_DEM_M_SIZEOF_ADMINDATA_CYCLECOUNTER()
{
  var Dem_Cfg_AdminData = Struct("Dem_Cfg_AdminData")
  if (Dem_Cfg_AdminData.HasMember("CycleCounter"))
  {
    var Dem_Cfg_AdminData_CycleCounter = Dem_Cfg_AdminData.GetMemberAsArray("CycleCounter")
    var DEM_CFG_GLOBAL_CYCLE_COUNT = Define("DEM_CFG_GLOBAL_CYCLE_COUNT")
    AssertArraySize(Dem_Cfg_AdminData_CycleCounter, Equal, DEM_CFG_GLOBAL_CYCLE_COUNT)
  }
  else
  {
    ReportError("Member CycleCounter of Dem_Cfg_AdminData is missing or renamed")
  }

}

/**********************************************************************************************************************
* Name         : CM_DEM_M_SIZEOF_DEBOUNCEDATA
* Parameter    : None
* Return value : None
* Description  : Verify Dem_Cfg_DebounceData[] is defined for each id in range [0..DEM_CFG_GLOBAL_DEBOUNCE_STORAGE_EVENT_COUNT[
* Requirements :
**********************************************************************************************************************/
def CM_DEM_M_SIZEOF_DEBOUNCEDATA()
{
  if (SymbolExists("Dem_Cfg_DebounceData"))
  {
    var Dem_Cfg_DebounceData = Array("Dem_Cfg_DebounceData")
    var DEM_CFG_GLOBAL_DEBOUNCE_STORAGE_EVENT_COUNT = Define("DEM_CFG_GLOBAL_DEBOUNCE_STORAGE_EVENT_COUNT")
    AssertArraySize(Dem_Cfg_DebounceData, Equal, DEM_CFG_GLOBAL_DEBOUNCE_STORAGE_EVENT_COUNT)
  }
  else if(IsStdOff("DEM_CFG_SUPPORT_DEBOUNCE_NV"))
  {
    // Feature is disabled, no check needed
  }
  else
  {
    ReportError("Dem_Cfg_DebounceData is not generated although DEM_CFG_SUPPORT_DEBOUNCE_NV is ON or DEM_CFG_SUPPORT_DEBOUNCE_NV does not exist.")
  }
}

/**********************************************************************************************************************
* Name         : CM_DEM_M_SIZEOF_PRESTORAGE
* Parameter    : None
* Return value : None
* Description  : Verify Dem_Cfg_Prestorage[] is defined for each id in range [0..DEM_CFG_GLOBAL_PRESTORED_FF_SIZE]
* Requirements :
**********************************************************************************************************************/
def CM_DEM_M_SIZEOF_PRESTORAGE()
{
  if (SymbolExists("Dem_Cfg_Prestorage"))
  {
    var Dem_Cfg_Prestorage = Array("Dem_Cfg_Prestorage")
    var DEM_CFG_GLOBAL_PRESTORED_FF_SIZE = Define("DEM_CFG_GLOBAL_PRESTORED_FF_SIZE")

