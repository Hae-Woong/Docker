/**********************************************************************************************************************
* Name         : CheckTimeSeriesSamplingProfileTableElement
* Parameter    : array - the array object for which the function is called
* Parameter    : index - the index of the current array element
* Parameter    : value - the value of the current array element
* Return value : None
* Description  : Callback function that verifies for an element in array Dem_Cfg_TimeSeriesSnapshotData
*                the first dimension of the array referenced by SnapshotDataPtrOfTimeSeriesSnapshotData is valid for values in range [0..MaxNumSamplesOfTimeSeriesSnapshotData[
*                the second dimension of the array referenced by SnapshotDataPtrOfTimeSeriesSnapshotData has at least MaxSnapshotSizeOfTimeSeriesSnapshotData elements
* Requirements : N/A
**********************************************************************************************************************/
def CheckTimeSeriesSamplingProfileTableElement(array, index, value)
{
  var TimeSeriesSamplingProfileRingBufferPtrString = value.GetMemberAsString("RingBufferPtrOfTimeSeriesSamplingProfileTable")
  var InnerArraySize = value.GetMemberAsNumber("SampleSizeOfTimeSeriesSamplingProfileTable");
  var PastSamples = value.GetMemberAsNumber("PastSamplesOfTimeSeriesSamplingProfileTable");
  var OuterSize

  if(PastSamples == 0)
  {
    OuterSize = 1
  }
  else
  {
    OuterSize = PastSamples
  }

  var StructName = GetStructNameFromStructMemberReferenceString(TimeSeriesSamplingProfileRingBufferPtrString)
  CheckVariableInDemNameSpace(StructName)

  var RingBufferStruct = Struct(StructName)

  var memberName = GetMemberNameFromStructMemberReferenceString(TimeSeriesSamplingProfileRingBufferPtrString)
  var RingBufferMemberBuffer = RingBufferStruct.GetMemberAsArray(memberName)
  var InnerArray = RingBufferMemberBuffer.GetElemAsArray(0)

  AssertArraySize(RingBufferMemberBuffer, Equal, OuterSize)
  AssertArraySize(InnerArray, Equal, InnerArraySize)
}

/**********************************************************************************************************************
* Name         : CM_DEM_M_SIZEOF_RINGBUFFER
* Parameter    : None
* Return value : None
* Description  : Verify that the buffer member of Dem_Cfg_RingBufferType pointed to by each element in the time series sampling profile table 
*   Dem_Cfg_TimeSeriesSamplingProfileTable is valid for indices in range specified by the relevant entries in the sampling profile table.
*   I.e. verify that the inner array size is SampleSizeOfTimeSeriesSamplingProfileTable and the outer array
*   size is PastSamplesOfTimeSeriesSamplingProfileTable or 1, if PastSamplesOfTimeSeriesSamplingProfileTable is 0. 
* Requirements :
**********************************************************************************************************************/
def CM_DEM_M_SIZEOF_RINGBUFFER()
{
  if(SymbolExists("Dem_Cfg_RingBuffer") && SymbolExists("Dem_Cfg_TimeSeriesSamplingProfileTable"))
  {
    var Dem_Cfg_TimeSeriesSamplingProfileTable = Array("Dem_Cfg_TimeSeriesSamplingProfileTable")
    var Dem_Cfg_RingBuffer = Struct("Dem_Cfg_RingBuffer")
    Dem_Cfg_TimeSeriesSamplingProfileTable.ForEachAsStruct(CheckTimeSeriesSamplingProfileTableElement)
    var DEM_CFG_TIMESERIES_NUMBER_OF_SAMPLING_PROFILES = Define("DEM_CFG_TIMESERIES_NUMBER_OF_SAMPLING_PROFILES")
    AssertArraySize(Dem_Cfg_TimeSeriesSamplingProfileTable, Equal, DEM_CFG_TIMESERIES_NUMBER_OF_SAMPLING_PROFILES)
  }
  else if (IsStdOff("DEM_CFG_SUPPORT_TIME_SERIES_PAST") && IsStdOff("DEM_CFG_SUPPORT_TIME_SERIES_FUTURE")) // Note: We expect the buffer to be generated even for future only time series since a single slot ring buffer is still used to keep track of the latest sample.
  {
    // Feature is disabled, no check needed
  }
  else
  {
    ReportError("Dem_Cfg_RingBuffer or Dem_Cfg_TimeSeriesSamplingProfileTable not generated although DEM_CFG_SUPPORT_TIME_SERIES_PAST is ON or DEM_CFG_SUPPORT_TIME_SERIES_PAST does not exist")
  }
}

