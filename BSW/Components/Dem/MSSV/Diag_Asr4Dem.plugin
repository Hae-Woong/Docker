/**********************************************************************************************************************
* Name         : CM_DEM_M_SIZEOF_TIMESERIES_BASEENTRY_SETINDEX
* Parameter    : None
* Return value : None
* Description  : For each BaseEntry referenced by BaseEntryOfTimeSeriesEntryTable in Dem_Cfg_TimeSeriesEntryTable verify:
            - that BaseEntry.NextSetId[] and BaseEntry.AllSetsStored[] are defined for indices in the range
              [0.. DEM_CFG_TIMESERIES_MAX_NUMBER_OF_SERIES[
* Requirements :
**********************************************************************************************************************/
def CM_DEM_M_SIZEOF_TIMESERIES_BASEENTRY_SETINDEX()
{
  if (SymbolExists("Dem_Cfg_TimeSeriesEntryTable"))
  {
      var Dem_Cfg_TimeSeriesEntryTable = Array("Dem_Cfg_TimeSeriesEntryTable")
      Dem_Cfg_TimeSeriesEntryTable.ForEachAsStruct(fun(array, index, value) {
      var TimeSeriesBaseEntryPtrString = value.GetMemberAsString("BaseEntryOfTimeSeriesEntryTable")

      var BaseEntry
      try {
        BaseEntry = GetBaseEntryFromTimeSeriesBaseEntryPtrString(TimeSeriesBaseEntryPtrString, false)
      } catch (e) {
        return // Error already reported by CM_DEM_M_SIZEOF_TIMESERIES_BASEENTRY_UNUSEDSAMPLES
      }

      var DEM_CFG_TIMESERIES_MAX_NUMBER_OF_SERIES = Define("DEM_CFG_TIMESERIES_MAX_NUMBER_OF_SERIES")

      if (BaseEntry.HasMember("NextSetId"))
      {
        var NextSetId = BaseEntry.GetMemberAsArray("NextSetId")
        AssertArraySize(NextSetId, Equal, DEM_CFG_TIMESERIES_MAX_NUMBER_OF_SERIES)
      }
      else
      {
        ReportError("Mandatory member NextSetId of BaseEntry has not been generated")
      }

      if (BaseEntry.HasMember("AllSetsStored"))
      {
        var AllSetsStored = BaseEntry.GetMemberAsArray("AllSetsStored")
        AssertArraySize(AllSetsStored, Equal, DEM_CFG_TIMESERIES_MAX_NUMBER_OF_SERIES)
      }
      else
      {
        ReportError("Mandatory member AllSetsStored of BaseEntry has not been generated")
      }

    })
  }
  else if (IsStdOff("DEM_CFG_SUPPORT_TIME_SERIES_PAST") && IsStdOff("DEM_CFG_SUPPORT_TIME_SERIES_FUTURE"))
  {
    // Feature disabled, no check needed
  }
  else
  {
     // Error already reported by CM_DEM_M_SIZEOF_TIMESERIES_BASEENTRY_UNUSEDSAMPLES
  }
}

