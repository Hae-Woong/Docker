    var Dem_Cfg_AdminData_MemoryOverflow = Dem_Cfg_AdminData.GetMemberAsArray("MemoryOverflow")
    var DEM_CFG_MAX_SIZE_OVERFLOWINDICATOR = Define("DEM_CFG_MAX_SIZE_OVERFLOWINDICATOR")
    AssertArraySize(Dem_Cfg_AdminData_MemoryOverflow, Equal, DEM_CFG_MAX_SIZE_OVERFLOWINDICATOR)
  }
  else if (IsStdOff("DEM_CFG_DATA_OVFLIND") && IsStdOff("DEM_CFG_SUPPORT_OVFLIND_API"))
  {
    // Feature disabled, no check needed
  }
  else
  {
    ReportError("Dem_Cfg_AdminData.MemoryOverflow not generated although DEM_CFG_DATA_OVFLIND OR DEM_CFG_SUPPORT_OVFLIND_API is ON or one of the PP switches does not exist.")
  }
}

