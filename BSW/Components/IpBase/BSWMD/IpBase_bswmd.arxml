					<CONTAINERS>
						<ECUC-PARAM-CONF-CONTAINER-DEF UUID="2fea153c-34d4-40d7-8e41-bbde15b2b4a7">
							<SHORT-NAME>IpBaseGeneral</SHORT-NAME>
							<ADMIN-DATA>
								<SDGS>
									<SDG GID="DV:CfgPostBuild">
										<SD GID="DV:postBuildSelectableChangeable">false</SD>
										<SD GID="DV:postBuildNotDeletable">false</SD>
									</SDG>
								</SDGS>
							</ADMIN-DATA>
							<LOWER-MULTIPLICITY>1</LOWER-MULTIPLICITY>
							<UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
							<POST-BUILD-CHANGEABLE>false</POST-BUILD-CHANGEABLE>
							<PARAMETERS>
								<!-- PARAMETER DEFINITION: IpBaseDevErrorDetect -->
								<ECUC-BOOLEAN-PARAM-DEF UUID="52d891f1-e899-4802-8f15-ce1502a8b1be">
									<SHORT-NAME>IpBaseDevErrorReport</SHORT-NAME>
									<DESC>
										<L-2 L="EN">If true then IpBase will enable the error-reporting to the Development Error Tracer (DET).</L-2>
									</DESC>
									<LOWER-MULTIPLICITY>1</LOWER-MULTIPLICITY>
									<UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
									<IMPLEMENTATION-CONFIG-CLASSES>
										<ECUC-IMPLEMENTATION-CONFIGURATION-CLASS>
											<CONFIG-CLASS>PRE-COMPILE</CONFIG-CLASS>
											<CONFIG-VARIANT>VARIANT-PRE-COMPILE</CONFIG-VARIANT>
										</ECUC-IMPLEMENTATION-CONFIGURATION-CLASS>
									</IMPLEMENTATION-CONFIG-CLASSES>
									<ORIGIN>Vector Informatik</ORIGIN>
									<REQUIRES-INDEX>false</REQUIRES-INDEX>
									<SYMBOLIC-NAME-VALUE>false</SYMBOLIC-NAME-VALUE>
									<DEFAULT-VALUE>true</DEFAULT-VALUE>
								</ECUC-BOOLEAN-PARAM-DEF>
								<!-- PARAMETER DEFINITION: IpBaseEnableInlineFunctions -->
								<ECUC-BOOLEAN-PARAM-DEF UUID="52d891f1-e899-4802-8f15-ce1502a8b1bd">
									<SHORT-NAME>IpBaseEnableInlineFunctions</SHORT-NAME>
									<DESC>
										<L-2 L="EN">If true then IpBase will enable inline functions for Byte swap, put and get macros and internal functions. Can be switched off to ease white box testing and debugging.</L-2>
									</DESC>
									<LOWER-MULTIPLICITY>1</LOWER-MULTIPLICITY>
									<UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
									<IMPLEMENTATION-CONFIG-CLASSES>
										<ECUC-IMPLEMENTATION-CONFIGURATION-CLASS>
											<CONFIG-CLASS>PRE-COMPILE</CONFIG-CLASS>
											<CONFIG-VARIANT>VARIANT-PRE-COMPILE</CONFIG-VARIANT>
										</ECUC-IMPLEMENTATION-CONFIGURATION-CLASS>
									</IMPLEMENTATION-CONFIG-CLASSES>
									<ORIGIN>Vector Informatik</ORIGIN>
									<REQUIRES-INDEX>false</REQUIRES-INDEX>
									<SYMBOLIC-NAME-VALUE>false</SYMBOLIC-NAME-VALUE>
									<DEFAULT-VALUE>true</DEFAULT-VALUE>
								</ECUC-BOOLEAN-PARAM-DEF>
								<!-- PARAMETER DEFINITION: IpBaseOneStepChecksumCopy -->
								<ECUC-BOOLEAN-PARAM-DEF UUID="52d891f1-e899-4802-8f15-ce1502a8b1ce">
									<SHORT-NAME>IpBaseOneStepChecksumCopy</SHORT-NAME>
									<DESC>
										<L-2 L="EN">If true then IpBase will copy and checksum calculation in one step. Enabling will speed up the calculation but increase the code size (ROM). Switching off may help in case of memory alignment issues.</L-2>
									</DESC>
									<LOWER-MULTIPLICITY>1</LOWER-MULTIPLICITY>
									<UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
									<IMPLEMENTATION-CONFIG-CLASSES>
										<ECUC-IMPLEMENTATION-CONFIGURATION-CLASS>
											<CONFIG-CLASS>PRE-COMPILE</CONFIG-CLASS>
											<CONFIG-VARIANT>VARIANT-PRE-COMPILE</CONFIG-VARIANT>
										</ECUC-IMPLEMENTATION-CONFIGURATION-CLASS>
									</IMPLEMENTATION-CONFIG-CLASSES>
									<ORIGIN>Vector Informatik</ORIGIN>
									<REQUIRES-INDEX>false</REQUIRES-INDEX>
									<SYMBOLIC-NAME-VALUE>false</SYMBOLIC-NAME-VALUE>
									<DEFAULT-VALUE>false</DEFAULT-VALUE>
								</ECUC-BOOLEAN-PARAM-DEF>
								<!-- PARAMETER DEFINITION: IpBaseSizeOfRamPointer -->
								<ECUC-ENUMERATION-PARAM-DEF UUID="e4d9d1de-c2ea-471f-9be9-c30941b6ed05">
									<SHORT-NAME>IpBaseSizeOfRamPointer</SHORT-NAME>
									<DESC>
										<L-2 L="EN">Configure the bit size of RAM pointers used for the UDP and TCP data type for the checksumm calculation. Pointer size equals the CPU register size on most platforms.</L-2>
									</DESC>
									<LOWER-MULTIPLICITY>1</LOWER-MULTIPLICITY>
									<UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
									<IMPLEMENTATION-CONFIG-CLASSES>
										<ECUC-IMPLEMENTATION-CONFIGURATION-CLASS>
											<CONFIG-CLASS>PRE-COMPILE</CONFIG-CLASS>
											<CONFIG-VARIANT>VARIANT-PRE-COMPILE</CONFIG-VARIANT>
										</ECUC-IMPLEMENTATION-CONFIGURATION-CLASS>
									</IMPLEMENTATION-CONFIG-CLASSES>
									<ORIGIN>Vector Informatik</ORIGIN>
									<REQUIRES-INDEX>false</REQUIRES-INDEX>
									<SYMBOLIC-NAME-VALUE>false</SYMBOLIC-NAME-VALUE>
									<DEFAULT-VALUE>uint32</DEFAULT-VALUE>
									<LITERALS>
										<ECUC-ENUMERATION-LITERAL-DEF UUID="aae2057f-5557-467a-bbaa-2e2389bbbe10">
											<SHORT-NAME>uint32</SHORT-NAME>
