<Settings>
	<Settings Name="com.vector.cfg.gen.Tresos_proxy.settings.TresosSettings">
		<Setting Name="TresosFolder" Value="$(SipRootPath)\ThirdParty\Mcal_Tc3xx\Supply\Tresos"/>
		<Setting Name="RequiredAsVersion" Value="4.4.0"/>
		<!-- Since 3.01.00 - Optional: To export EcuC file with a specific ASR schema version -->
		<Setting Name="SpecificAsVersionRequired" Value="true"/>
		<!-- Optional: Set to true to export EcuC file with a specific ASR schema version -->
		<Setting Name="IgnoreTresosMsgIDs" Value="1648,1055,1072"/>
		<!-- Optional: To disable printing of any messages with a specific message id. Several ids can be passed comma separated. For each message id a commandline option -D<msgId>=false is passed to the tresos executable. -->
		<Setting Name="GenerateModulesAtOnce" Value="true"/>
		<!-- Since 3.05.00 - Optional: Set true to allow performance optimization by generating all MCAL modules in one step (check box in CFG5 settings) -->
		<Setting Name="TransformationXsltFile" Value="$(SipRootPath)\Components\Mcal_Tc3xx\GeneratorMsr\prepare_output_TC3xx.xsl"/>
		<!-- Precondition: one XSL script used by all modules -->
		<Setting Name="GeneratePostBuildVariants" Value="false"/>
		<!-- Since 5.00.00 - Optional: Set true to allow generation of PostBuild Variants (check box in CFG5 settings) -->
	</Settings>
</Settings>
