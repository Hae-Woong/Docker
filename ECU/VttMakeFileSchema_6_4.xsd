  <xs:simpleType name="VsTargetVersionType">
    <xs:annotation>
      <xs:documentation>
        <![CDATA[
        <p>Visual Studio version [VS2017, VS2019, VS2022].</p>
        <p>[Default] Version set as default in vVIRTUALtarget options.</p>
        ]]>
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="VS2017">
        <xs:annotation>
          <xs:documentation>
            <![CDATA[
            Visual Studio 2017, any edition
            ]]>
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="VS2019">
        <xs:annotation>
          <xs:documentation>
            <![CDATA[
            Visual Studio 2019, any edition
            ]]>
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="VS2022">
        <xs:annotation>
          <xs:documentation>
            <![CDATA[
            Visual Studio 2022, any edition
            ]]>
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="TargetPlatformType">
    <xs:annotation>
      <xs:documentation>
        <![CDATA[
        <p>Target Platform [Windows_x86, Windows_x64, Linux_x64].</p>
        <p>Note that <strong>Windows_x64</strong> is currently only supported with Visual Studio compiler and <strong>Linux_x64</strong> is only supported with <a href="#../../GenerateCMakeProject">GenerateCMakeProject</a>.</p>
        <p>With <strong>GenerateVcxProject</strong>, the target platform is optional and defaults to the target platform setting in the vVIRTUALtarget options.</p>
        <p>
          With <strong>GenerateCMakeProject</strong>, the target platform is optional and no default setting of vVIRTUALtarget is used. Instead, a VTTMAKE file without a target platform is expected to be <strong>generic</strong> 
          so that it can be used on both Windows and Linux. In this case, building the SUT is skipped by <strong>VttMake</strong> and must then be done manually.
        </p>
        ]]>
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="Windows_x86"/>
      <xs:enumeration value="Windows_x64"/>
      <xs:enumeration value="Linux_x64"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="SourceFileSet">
    <xs:annotation>
      <xs:documentation>
        <![CDATA[
        <p>Set of files. Can be relative or absolute, depending on the used sub-elements (<a href="#File">File</a> or <a href="#SipRelativeFile">SipRelativeFile</a>).</p>
        <p>Files can be virtually grouped by <a href="#Filter">Filter</a>.</p>
        <p>Supported file types are .c, .h, .cpp, .hpp.</p>
        ]]>
      </xs:documentation>
    </xs:annotation>
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element minOccurs="0" maxOccurs="unbounded" name="SipRelativeFile" type="xs:string">
        <xs:annotation>
          <xs:documentation>
            <![CDATA[
            <p>File path relative to the BSW Package directory.</p>
            <p>Supported file types are .c, .h, .cpp, .hpp.</p>
            <p><strong>Note</strong>: It is also possible to use environment variables e.g. <strong>path2\\%envVar1%\\file.c</strong>.</p>
            <p><strong>Note</strong>: If you are building under Windows for Linux, then the relative file references are not references to the copied BSW Package. You must clone your source file structure under Linux.</p>
            ]]>
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="File" type="xs:string">
        <xs:annotation>
          <xs:documentation>
            <![CDATA[
            <p>File path relative to the build root directory (<a href="#/BuildSpecification/Settings/RootDir">RootDir</a>) or absolute path.</p>
            <p>Supported file types are .c, .h, .cpp, .hpp</p>
            <p><strong>Note</strong>: It is also possible to use environment variables e.g. path2\\%envVar1%\\file.c</p>
            ]]>
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="Filter" type="Filter" />
    </xs:choice>
  </xs:complexType>
  <xs:complexType name="CMakeSourceFileSet">
    <xs:annotation>
      <xs:documentation>
        <![CDATA[
        <p>Set of files. Can be relative or absolute, depending on the used sub-elements (<a href="#File">File</a> or <a href="#SipRelativeFile">SipRelativeFile</a>).</p>
        <p>Supported file types are .c, .h, .cpp, .hpp.</p>
        ]]>
      </xs:documentation>
    </xs:annotation>
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element minOccurs="0" maxOccurs="unbounded" name="SipRelativeFile" type="xs:string">
        <xs:annotation>
          <xs:documentation>
            <![CDATA[
            <p>File path relative to the BSW Package directory.</p>
            <p>Supported file types are .c, .h, .cpp, .hpp.</p>
            <p><strong>Note</strong>: It is also possible to use environment variables e.g. <strong>path2\\%envVar1%\\file.c</strong>.</p>
            <p><strong>Note</strong>: If you are building under Windows for Linux, then the relative file references are not references to the copied BSW Package. You must clone your source file structure under Linux.</p>
            ]]>
